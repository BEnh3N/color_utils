{
  "schemaVersion": 1,
  "id": "color_utils",
  "version": "${version}",
  "name": "Color Utils",
  "description": "A mod to help builders find the perfect color for their next build",
  "authors": [
    "BEnh3N"
  ],
  "contact": {
    "homepage": "https://github.com/benh3n/color_utils",
    "issues": "https://github.com/BEnh3N/color_utils/issues"
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/color_utils/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "net.benh3n.color_utils.client.ColorUtilsClient"
    ],
    "main": [
      "net.benh3n.color_utils.ColorUtils"
    ]
  },
  "mixins": [
    "color_utils.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "custom": {
    "modmenu": {
      "update_checker": false
    }
  }
}
